.server {
  height: 100vh;
  background: url(https://static.gute.fun/static/luckyOwn/lucky/pageB/bj.png) no-repeat;
  background-size: cover;
}
.server .server_head {
  padding: 0 30rpx;
  margin-bottom: 127rpx;
}
.server .server_head image {
  width: 44rpx;
  height: 44rpx;
}
.server .server_head text {
  color: #FFFFFF;
  font-size: 36rpx;
  font-weight: bold;
}
.server .server_head text:last-child {
  width: 44rpx;
}
.server .server_main {
  flex-direction: column;
}
.server .server_main_box {
  width: 525rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  background: #F2F2F2;
  border-radius: 34rpx;
}
.server .server_main_box .server_main_head {
  width: 132rpx;
  height: 132rpx;
  margin-bottom: 32rpx;
  margin-top: -66rpx;
}
.server .server_main_box .server_main_head image {
  border-radius: 50%;
}
.server .server_main_box .server_main_name {
  display: flex;
  margin-bottom: 24rpx;
  flex-direction: column;
}
.server .server_main_box .server_main_name text:first-child {
  font-size: 46rpx;
  font-weight: bold;
}
.server .server_main_box .server_main_name text:last-child {
  color: #666666;
  font-size: 30rpx;
  margin-top: 23rpx;
}
.server .server_main_box .server_main_img {
  width: 430rpx;
  height: 430rpx;
}
.server .server_main_box .server_main_txt {
  color: #999999;
  font-size: 30rpx;
  padding: 20rpx 0 30rpx 0;
}
.server .server-download {
  width: 523rpx;
  height: 89rpx;
  margin-top: 30rpx;
  background: #FFFFFF;
  border-radius: 18rpx;
}
.server .server-download image {
  width: 42rpx;
  height: 42rpx;
  margin-right: 10rpx;
}
.server .server-download text {
  font-size: 30rpx;
  font-weight: bold;
}
.kf-container {
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-direction: column;
}
.kf-container .server_title {
  width: 100%;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 2021;
}
.kf-container .server_title .server_nav {
  width: 100%;
}
.kf-container .server_title .server-title_head {
  width: 100%;
  height: 88rpx;
  padding: 0 30rpx;
  background: #FFFFFF;
}
.kf-container .server_title .server-title_head .server_title_return {
  width: 44rpx;
  height: 44rpx;
}
.kf-container .server_title .server-title_head .server_title_name {
  width: 466rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 36rpx;
  font-weight: bold;
}
.kf-container .server_title .server-title_head .server_title_mp3 {
  width: 44rpx;
  height: 44rpx;
}
.kf-container ._iframe {
  flex: 1;
  border: 0;
  height: 100%;
  width: 100%;
}

