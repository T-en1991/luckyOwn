.address_head {
  width: 100%;
  height: 88rpx;
  padding: 0 30rpx;
  background: #FFFFFF;
}
.address_head image {
  width: 44rpx;
  height: 44rpx;
}
.address_head text {
  font-size: 36rpx;
  font-weight: bold;
}
.address_ul {
  padding: 20rpx 30rpx 0 30rpx;
}
.address_ul .address_li .address_name {
  font-size: 30rpx;
  font-weight: bold;
  padding: 30rpx 0;
}
.address_ul .address_li .address_ipt {
  height: 80rpx;
  padding: 0 30rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
}
.address_ul .address_li .address_ipt input {
  font-size: 28rpx;
}
.address_ul .address_li .address_ipt image {
  width: 22rpx;
  height: 22rpx;
}
.address_ul .address_li .address_ipts {
  padding: 30rpx;
  height: 160rpx;
}
.address_ul .address_li .address_ipts textarea {
  font-size: 28rpx;
}
.address_switch {
  height: 88rpx;
  margin: 30rpx 0 84rpx 0;
  padding: 0 30rpx;
  background: #FFFFFF;
}
.address_switch text {
  font-size: 30rpx;
  font-weight: bold;
}
.address_btn {
  width: 690rpx;
  height: 98rpx;
  color: #333333;
  margin: 0 auto;
  font-size: 30rpx;
  font-weight: bold;
  background: #FFFFFF;
  box-shadow: 0rpx 0rpx 121rpx 0rpx rgba(63, 52, 2, 0.12);
  border-radius: 10rpx;
}

