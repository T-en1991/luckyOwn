.address_wu {
  display: flex;
  padding: 165rpx 30rpx 35rpx 30rpx;
  flex-direction: column;
  justify-content: space-between;
  height: calc(100vh - 44px);
}
.address_wu .address_wu_img {
  flex-direction: column;
}
.address_wu .address_wu_img image {
  width: 354rpx;
  height: 310rpx;
  margin-bottom: 50rpx;
}
.address_wu .address_wu_img text {
  font-size: 28rpx;
  font-weight: bold;
}
.address_wu_btn {
  height: 98rpx;
  color: #333333;
  font-size: 30rpx;
  font-weight: bold;
  background: #FFFFFF;
  box-shadow: 0rpx 0rpx 121rpx 0rpx rgba(63, 52, 2, 0.12);
  border-radius: 10rpx;
}
.address_have_ul {
  padding: 30rpx 30rpx 130rpx 30rpx;
}
.address_have_ul .address_have_li {
  padding: 30rpx;
  margin-bottom: 30rpx;
  background: #FFFFFF;
}
.address_have_ul .address_have_li .address_have_li_title {
  color: #666666;
  font-size: 28rpx;
}
.address_have_ul .address_have_li .address_have_li_txt {
  font-size: 26rpx;
  margin: 20rpx 0;
}
.address_have_ul .address_have_li .address_have_li_name text {
  color: #999999;
  font-size: 30rpx;
}
.address_have_ul .address_have_li .address_have_li_name text:last-child {
  font-size: 28rpx;
  margin-left: 20rpx;
}
.address_have_ul .address_have_li .address_have_li_edit {
  width: 44rpx;
  height: 44rpx;
}
.address_have_btn {
  width: 690rpx;
  left: 50%;
  bottom: 34rpx;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  position: fixed;
}

